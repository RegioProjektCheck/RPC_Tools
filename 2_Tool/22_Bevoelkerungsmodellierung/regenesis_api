import urllib2, json
from pprint import pprint
url = "http://api.regenesis.pudo.org/cube/"
table = "52411kj002/aggregate?"
parameter = "cut=jahr.text:2011"
query = url + table
queryresult = urllib2.urlopen(query,parameter)
jsonContent = queryresult.read()
json_data=open(jsonContent)
data = json.load(json_data)
pprint(data)
